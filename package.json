{
  "name": "portfolio",
  "description": "Personal Portfolio",
  "version": "1.0.0",
  "author": "Alvina Yang <alvina.yang@mail.utoronto.ca>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alvina-yang/alvina-yang.github.io"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && gh-pages -d public",
    "win": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm run start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@splidejs/react-splide": "^0.7.12",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
